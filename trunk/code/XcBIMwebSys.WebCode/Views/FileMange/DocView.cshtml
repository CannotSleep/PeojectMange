
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Title</title>
</head>
<body id="mybody">

    <!---<iframe id="myframe" isContentEditable="true" width="100%;" height="100%" src="~/Temp/username/材料表.xlsx"></iframe>-->

    <script src="~/Content/js/jquery.min.js" type="text/javascript" charset="utf-8"></script>

    <script type="text/javascript">

        var htmla = "";

        $(document).ready(function () {
            var docName = getCookie('docname');
            //alert(docName);
            //alert(picName);
            htmla += "<iframe id='myframe' style='display: none'  isContentEditable='true' width='100%' height='100%' src='" + "/Temp/123654/" + docName + "'></iframe>";
            $("#mybody").html(htmla);
        });



        //读取Cookie的方法
        function getCookie(key) {
            var arr1 = document.cookie.split('; ');
            for (var i = 0; i < arr1.length; i++) {
                var arr2 = arr1[i].split('=');
                if (arr2[0] == key) {
                    return decodeURI(arr2[1]);
                }
            }
        }

    </script>

</body>

</html>
